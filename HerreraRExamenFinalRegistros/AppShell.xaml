<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="HerreraRExamenFinalRegistros.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:HerreraRExamenFinalRegistros"
    Shell.FlyoutBehavior="Disabled"
    xmlns:views="clr-namespace:HerreraRExamenFinalRegistros.View"
    Title="HerreraRExamenFinalRegistros">
    
    <TabBar>
        <ShellContent
            Title="Registrar"
            ContentTemplate="{DataTemplate views:RegistroPage}" />

        <ShellContent
            Title="Proyectos BD"
            ContentTemplate="{DataTemplate views:ListaBDPage}" />

        <ShellContent
            Title="Logs"
            ContentTemplate="{DataTemplate views:ListaLogPage}" />
    </TabBar>

</Shell>
