<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HerreraRExamenFinalRegistros.View.ListaLogPage"
             xmlns:vm="clr-namespace:HerreraRExamenFinalRegistros.ViewModels"
             Title="Lista de los Proyectos en Log">

    <ContentPage.BindingContext>
        <vm:ListaLogsViewModel />
    </ContentPage.BindingContext>

    <CollectionView ItemsSource="{Binding Logs}">
        <CollectionView.ItemTemplate>
            <DataTemplate>
                <Frame BorderColor="LightGray" Padding="10" Margin="5">
                    <Label Text="{Binding Descripcion}" />
                </Frame>
            </DataTemplate>
        </CollectionView.ItemTemplate>
    </CollectionView>
</ContentPage>